[[workflows.workflow]]
name = "Complete Fresh Start"
author = 38337401
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "pkill -f \"tsx server/index.ts\" || true"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "pkill -f \"npm run dev\" || true"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "sleep 2"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run dev"

